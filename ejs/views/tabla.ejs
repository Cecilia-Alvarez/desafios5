<header>
    <%- include('./partials/encabezado.ejs') %>
</header>
<body>
  <div class="container mt-3">
    <div class="jumbotron">
      <h1 style="color: blue">Productos Ingresados</h1>
      <br />
<% if(productos.length !== 0) { %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr style="color: yellow;"> <th>Nombre</th> <th>Autor</th> <th>Precio</th> </tr>
           <% for (const producto in productos ) { %>
            <tr>
                <td> <%= productos[producto].nombre %> </td>
                <td> <%= productos[producto].autor %> </td>
                <td> <%= productos[producto].precio %></td>
            </tr>
            <% } %>
        </table>
    </div>
    <div>
        <button class="btn btn-success mt-3 mb-5">
            <a style="color: white" href="/">Volver</a>
        </button>
    </div>
<% } else{ %> 
   <h3 style="color: black">No existen datos</h3>
<% }%>
</div>
</body>

